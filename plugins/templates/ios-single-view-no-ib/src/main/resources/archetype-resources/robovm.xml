#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<config>
    <executableName>${symbol_dollar}{app.executable}</executableName>
    <mainClass>${symbol_dollar}{app.mainclass}</mainClass>
    <os>ios</os>    
    <target>ios</target>
    <iosInfoPList>Info.plist.xml</iosInfoPList>
    <resources>
        <resource>
            <directory>resources</directory>
        </resource>
    </resources>
    <!-- Remove comment to automatically skip compilation and linking in IDEA -->
    <!-- <smartSkipRebuild>true</smartSkipRebuild> -->
</config>